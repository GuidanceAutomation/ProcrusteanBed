<UserControl x:Class="ProcrusteanBed.Core.Controls.AtomicMoveControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ProcrusteanBed.Core.Controls"
             xmlns:toolkit="http://schemas.xceed.com/wpf/xaml/toolkit"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <StatusBar>Atomic Move</StatusBar>
        <TextBlock>Move Id</TextBlock>
        <toolkit:IntegerUpDown Value="{Binding Path=MapItemId}"/>
    </StackPanel>
</UserControl>
